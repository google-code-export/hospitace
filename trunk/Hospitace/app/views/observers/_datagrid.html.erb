<div id="datagrid">
<table class="zebra-striped">
  <thead>
  <tr>
    <th>#</th>
    <th>Login</th>
    <th>Name</th>
    <th>Roles</th>
    <th></th>
  </tr>
  </thead>
  

<% @observers.each do |observer| %>
  <%
  user = observer.user %>
  <%#= debug user %>
  <tr>
    <td><%= user.id %></td>
    <td><%= user.login %></td>
    <td><%= user.full_name %></td>
    <td><%= user.roles_mask %></td>
    <td><%= yield observer %></td>
  </tr>
<% end %>
</tbody>
</table>
</div>